{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-essential",
		"prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
		"prettier"
    ],
    "rules": {
		"prettier/prettier": ["error", {}]
    }
}
